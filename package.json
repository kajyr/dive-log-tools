{
  "name": "dive-log-importer",
  "version": "4.3.4",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "repository": "https://github.com/kajyr/dive-log-importer",
  "author": "Carlo Panzi <carlo.panzi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && rollup -c rollup.config.js",
    "watch": "rollup -w -c rollup.config.js",
    "test": "jest --verbose",
    "test-run": "ts-node  --project ./node.tsconfig.json ./cli.ts",
    "prepublishOnly": "yarn build",
    "postversion": "git push origin && git push origin --tags",
    "changelog": "changelog",
    "postchangelog": "git add CHANGELOG.md && git commit -m 'chore: updated CHANGELOG.md' ",
    "generate-schema": "typescript-json-schema --required src/types.ts Logbook > dive-schema.json",
    "release": "release-it",
    "prerelease": "npm run build 66 npm run changelog"
  },
  "dependencies": {
    "date-fns": "^2.26.0",
    "fast-xml-parser": "^3.21.1",
    "release-it": "^14.11.8",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "ajv": "^8.8.2",
    "generate-changelog": "^1.8.0",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "typescript-json-schema": "^0.52.0"
  }
}
