{
  "name": "logbook-templates",
  "version": "2.7.1",
  "description": "Un template per DivingLog e MacDive per stampare i [logbook F.I.P.S.A.S.][fipsas] precompilati",
  "type": "commonjs",
  "scripts": {
    "build": "ts-node cli.ts --dest=build/export.pdf",
    "build:empty": "ts-node cli.ts -vl --dest=build/export_empty.pdf --empty",
    "watch": "nodemon cli.ts -l --debug --dest=build/export.pdf",
    "test:demo": "npm run build:empty"
  },
  "repository": "https://github.com/kajyr/dive-log-tools/templates",
  "author": "Carlo 'kajyr' <carlo.panzi@gmail.com>",
  "license": "ISC",
  "bin": {
    "logbook": "ts-node ./cli.ts"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "dive-log-importer": "^4.3.5",
    "fs-extra": "^11.0.0",
    "glob": "^8.0.3",
    "home-config": "^0.1.0",
    "mclip": "^2.1.1",
    "nodemon": "^2.0.15",
    "pdfkit": "^0.13.0",
    "tmp": "0.2.1",
    "ts-node": "^10.9.1",
    "xml2js": "^0.4.23"
  },
  "nodemonConfig": {
    "delay": "250",
    "ignore": [
      "./demo",
      "./build",
      "./mdoelli"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/pdfkit": "^0.12.8"
  }
}
